<div class="row">
	<div class="span6">
		<%= form_tag followers_user_path, method: 'get' do %>
			<table cellpadding="5">
				<tr>
					<td>
						<%= text_field_tag :search, params[:search] %>
					</td>
					<td>
						<%= submit_tag "Search", name: nil,
						 id: "users_search_button", class: "btn btn-primary btn-align" %>
					</td>
				</tr>
			</table>			
		<% end %>
	</div>
</div>